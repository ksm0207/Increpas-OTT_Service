<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample">

<select id="netflix" resultType="com.ott.netflix.vo.NetflixVO" parameterType="java.util.Map">
	select *
	from ott_netflix
	where NF_IDX between #{begin} and #{end}
</select>

<select id="netflixTotal" resultType="int">
	select count(*)
	from ott_netflix
</select>


</mapper>